<div id="map-canvas" style='width: 100%; height: 400px;'></div>
<div id="chat">

</div>

<%= form_tag("", :class => "form-inline") do %>
	<%= text_field_tag :message, nil, :placeholder => "Write a message", :autocomplete => "off" %>
	<%= submit_tag("Send", :id => "chat-send") %>
<% end %>

<%= subscribe_to "/messages/new" %>