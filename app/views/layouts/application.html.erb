<!doctype html>
<html lang="en"><head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <title>OpenDataDay</title>
  <meta name="viewport" content="width=device-width, initial-scale=1"> 
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  markers = handler.addMarkers(<%=raw @hash.to_json %>);
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBgHHPlsYf0e6VPwD_t4koqbJfq-EdWOiE&sensor=true"
  type="text/javascript"></script>
</head>
<body>
    <script type="text/javascript">
      function initialize() {
        var mapOptions = {
          center: new google.maps.LatLng(56.166773, 14.889221),

          zoom: 14
        };
        var map = new google.maps.Map(document.getElementById("map-canvas"),
            mapOptions);
      }
      google.maps.event.addDomListener(window, 'load', initialize);
    </script>


<div class="demo-wrapper" data-role="page">
        <!-- panel left -->
        <div class="panel left" data-role="panel" data-position="left" data-display="push" id="panel-01">


        </div>
		<div class="header" data-role="header">
			<span class="open left"><a href="#panel-01">A</a></span>
            <span class="title">Chit Chat</span>
			<span class="open right"><a href="#panel-02">B</a></span>
		</div>
        <div class="content" data-role="content">
        <!-- body -->
        <div style='width: 800px;'>
  <div id="map-canvas" style='width: 800px; height: 400px;'></div>
</div>


        <%= yield %>

        </div>


    	<!-- panel right -->
    	<div class="panel right" data-role="panel" data-position="right" data-display="overlay" id="panel-02">

        
    	</div>
	</div>
</body>
</html>
